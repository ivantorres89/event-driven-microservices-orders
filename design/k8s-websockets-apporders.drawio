        <mxCell id="RMAeVSShHTEY-Qpviwvw-75" parent="1" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;rounded=0;" value="For ApplicationInsights &amp;amp; Monitoring:&amp;nbsp;&lt;span style=&quot;background-color: rgb(18, 18, 18);&quot;&gt;&lt;span style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;CorrelationId propagated across logs and traces&lt;/span&gt;&lt;br&gt;&lt;div style=&quot;&quot;&gt;&lt;div&gt;DLQ → Monitoring / Ops workflow&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;/div&gt;&lt;div style=&quot;font-size: 11px; text-wrap-mode: nowrap;&quot;&gt;&lt;br&gt;&lt;/div&gt;&lt;/span&gt;" vertex="1">
