# Local dev ports / HTTPS (used by docker-compose.yml)
ORDER_NOTIFICATION_HTTP_PORT=5006
ORDER_NOTIFICATION_HTTPS_PORT=5007

# order-accept is published on host :8081 (container :8080)
ORDER_ACCEPT_HTTP_PORT=8081

# Password used to export dotnet dev-certs into infra/local/certs/contoso-devcert.pfx
# DEV ONLY.
HTTPS_CERT_PASSWORD=contoso

# DEV-only JWT signing key (used by order-notification /dev/token and JWT validation)
# Must be 32+ characters.
DEVJWT_SIGNING_KEY=PLEASE_CHANGE_ME__DEV_ONLY__AT_LEAST_32_CHARS_LONG

# SQL Server (sa) password used by the local sqlserver container.
# Must satisfy SQL Server password complexity rules.
SA_PASSWORD=Your_strong_Password123!
